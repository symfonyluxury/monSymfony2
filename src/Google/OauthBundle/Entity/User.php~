<?php

namespace Google\OauthBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="user")
 */
class User
{

      /**
       * @ORM\Id
       * @ORM\Column(type="integer")
       * @ORM\GeneratedValue(strategy="AUTO")
       */
      protected $id;
      /** @ORM\Column(type="string")  **/
      protected $name;
      /** @ORM\Column(type="string")  **/
      protected $pwd;

      //put your code here
}

?>